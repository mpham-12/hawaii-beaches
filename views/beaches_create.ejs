<% layout('/layouts/boilerplate') %> 

  <h1>BEACHES CREATE FORM</h1>
  <form action="/beaches" method="POST">
  <input type="text" name="title" id="title" placeholder="title">
  <input type="text" name="location" id="location" placeholder="city, state">
  <input type="text" name="latitude" id="latitude" placeholder="latitude">
  <input type="text" name="longitude" id="longitude" placeholder="longitude">
  <textarea cols="50" rows="10"  name="description" id="description" placeholder="description"> </textarea>
  <button>Create</button>
  </form>
  <a href="/beaches">Back</a>
